{
  "name": "eslint-config-jane",
  "description": "Jane's Eslint config.",
  "version": "2.0.1",
  "author": "Zac Anger <zac@zacanger.com> (http://zacanger.com)",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "rules.js",
    "prettier.js",
    "helpers.js"
  ],
  "scripts": {
    "format": "prettier \"*.js\" --write",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run test",
    "test": "npm run test:lint && npm run test:test",
    "test:lint": "eslint -c .eslintrc.json rules.js",
    "test:test": "node test"
  },
  "homepage": "https://github.com/jane/eslint-config-jane#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jane/eslint-config-jane.git"
  },
  "bugs": {
    "url": "https://github.com/jane/eslint-config-jane/issues"
  },
  "keywords": [
    "config",
    "eslint",
    "jane"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "eslint": "4.19.1",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jest": "21.15.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-unicorn": "4.0.2",
    "husky": "0.14.3",
    "lint-staged": "7.0.4",
    "prettier": "1.12.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "peerDependencies": {
    "babel-eslint": "^8.0.0",
    "eslint": "^4.3.0",
    "eslint-plugin-babel": "^4.1.2 || ^5.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.2.0",
    "eslint-plugin-unicorn": "^4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
